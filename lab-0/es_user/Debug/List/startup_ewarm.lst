    300          // This function disables FPU initialization and sets up the PSP
    301          __weak void __iar_init_vfp(void);
    302          inline void __iar_init_vfp(void){
    303            asm("MOV R1, SP");
    304            asm("ADD R1, R1, #64"); // assuming stack size 128
    305          
    306            asm("MRS R0, CONTROL");
    307            asm("ORR R0, R0, #2");
    308            asm("MSR CONTROL, R0");
    309            asm("DSB");
    310            asm("ISB");
    311          
    312            asm("MOV SP, R1");
    313          }

